{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": false,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": false,
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES2021",
    "typeRoots": ["./node_modules/@types"]
  },
  "exclude": ["node_modules"]
}
